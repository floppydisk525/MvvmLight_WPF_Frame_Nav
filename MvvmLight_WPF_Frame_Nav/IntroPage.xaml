<Page x:Class="MvvmLight_WPF_Frame_Nav.IntroPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MvvmLight_WPF_Frame_Nav"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="IntroPage"
      DataContext="{Binding Main, Source={StaticResource Locator}}">

    <Grid>
        <TextBox Text="{Binding Screen1Text}" HorizontalAlignment="Left" Height="23" Margin="65,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="167"/>
        <TextBox Text="{Binding Screen2Text}" HorizontalAlignment="Left" Height="23" Margin="65,119,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="167"/>
        <TextBox Text="{Binding Screen3Text}" HorizontalAlignment="Left" Height="23" Margin="65,160,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="167"/>
        <TextBlock Text="IntroPage - Page 1" HorizontalAlignment="Left" Height="35" Margin="44,18,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209"/>
    </Grid>
</Page>

